<?xml version="1.0" encoding="utf-8"?>
<com.whomentors.sadajura.ui.CheckableLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context=".RecipientsActivity$RecipientsAdapter" >

    <LinearLayout android:id="@+id/thumbnail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="3dip"
        android:layout_alignParentLeft="true"
        android:layout_marginRight="5dip">
 
        <com.parse.ParseImageView
            android:id="@+id/profileImage"
            android:layout_width="45dip"
            android:layout_height="45dip"
            android:layout_marginBottom="3dp"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="3dp"
            android:src="@null" />
 
    </LinearLayout>
    
    <CheckedTextView
        android:id="@+id/senderLabel"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/thumbnail"
        android:drawableRight="?android:attr/textCheckMark"
        android:gravity="center_vertical"
        android:paddingRight="10dp"
        android:textAppearance="?android:attr/textAppearanceMedium" />

</com.whomentors.sadajura.ui.CheckableLinearLayout><!-- From: file:/C:/Users/Michael%20Yoon%20Huh/Development/Android/Sadajura/snap/src/main/res/layout/recipients_item.xml -->