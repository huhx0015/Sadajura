<?xml version="1.0" encoding="utf-8"?>
 <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".EditFriendsActivity$EditFriendsAdapter" >

    <LinearLayout android:id="@+id/thumbnail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="3dip"
        android:layout_alignParentLeft="true"
        android:layout_marginRight="5dip" >
 
        <com.parse.ParseImageView
            android:id="@+id/profileImage"
            android:layout_width="45dip"
            android:layout_height="45dip"
            android:layout_marginBottom="3dp"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="3dp"
            android:src="@null" />
 
    </LinearLayout>
    
	<TextView
	    android:id="@+id/senderLabel"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignParentTop="true"
	    android:layout_centerVertical="true"
	    android:layout_marginTop="16dp"
	    android:layout_toRightOf="@+id/thumbnail"
	    android:textAppearance="?android:attr/textAppearanceMedium" />

</RelativeLayout><!-- From: file:/C:/Users/Michael%20Yoon%20Huh/Development/Android/Sadajura/snap/src/main/res/layout/edit_friends_item.xml -->